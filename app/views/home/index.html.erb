<div class="container">
  <h1>학생정보</h1>
  
  <% @students.each do |student| %>
    <p>학생이름: <%= student.name %></p>
    <p>학생번호: <%= student.student_number %></p>
  <% end %>
  
  <form action="/home/upload" method="POST">
    <div class="form-group">
      <label for="exampleInputEmail1">학생이름</label>
      <input type="text" name="student_name" class="form-control" id="exampleInputEmail1" placeholder="학생이름">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">학생번호</label>
      <input type="text" name="student_number" class="form-control" id="exampleInputPassword1" placeholder="학생번호">
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>